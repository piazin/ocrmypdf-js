{
  "name": "ocrmypdf-js",
  "version": "1.1.1",
  "description": "Abstração da lib ocrmypdf",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "deploy": "npm run test && npm run build && npm publish --access public",
    "commit": "git-cz",
    "prepare": "husky",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/piazin/ocrmypdf-js.git"
  },
  "homepage": "https://github.com/piazin/ocrmypdf-js",
  "keywords": [
    "ocr",
    "pdf",
    "ocrmypdf",
    "parse"
  ],
  "author": "Lucas Souza",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@types/cli-color": "^2.0.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "cli-color": "^2.0.4"
  }
}
